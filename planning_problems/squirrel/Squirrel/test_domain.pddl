(define (domain find_key)
(:requirements :typing :conditional-effects :negative-preconditions :disjunctive-preconditions)

(:types
	pos obj col gar
	level
	state
	knowledgebase
)

(:predicates
	(color ?o - obj ?c - col ?s - state)
	(Rcolor ?o - obj ?c - col ?s - state)
	(trashed ?o - obj ?s - state)
	(Rtrashed ?o - obj ?s - state)
	(at ?p - pos ?s - state)
	(Rat ?p - pos ?s - state)
	(holding ?o - obj ?s - state)
	(Rholding ?o - obj ?s - state)
	(obj-at ?o - obj ?p - pos ?s - state)
	(Robj-at ?o - obj ?p - pos ?s - state)
	(garbage-color ?g - gar ?c - col)
	(garbage-at ?g - gar ?l - pos)
	(adj ?i ?j - pos)
	(part-of ?s - state ?kb - knowledgebase)
	(current_kb ?kb - knowledgebase)
	(parent ?kb ?kb2 - knowledgebase)

	;; Bookkeeping predicates.
	(next ?l ?l2 - level)
	(lev ?l - LEVEL)
	(m ?s - STATE)
	(stack ?s - STATE ?l - LEVEL)
	(resolve-axioms)
)

(:constants
	; All the balls.
	ball_0 - obj
	ball_1 - obj
	; The locations.
	loc_0 - pos
	loc_1 - pos
	; The colours.
	colour_0 - col
	colour_1 - col
	; The garbage places.
	colour_0_garbage - gar
	colour_1_garbage - gar
	; All the states.
	basic - state
	kb_location_ball_0_loc_0 - state
	kb_location_ball_0_loc_0_loc_1 - state
	kb_colour_ball_0_colour_0 - state
	kb_colour_ball_0_colour_0_colour_1 - state
	kb_location_ball_1_loc_0 - state
	kb_location_ball_1_loc_0_loc_1 - state
	kb_colour_ball_1_colour_0 - state
	kb_colour_ball_1_colour_0_colour_1 - state
	; The knowledge bases
	basis_kb - knowledgebase
	kb_location_ball_0 - knowledgebase
	kb_colour_ball_0 - knowledgebase
	kb_location_ball_1 - knowledgebase
	kb_colour_ball_1 - knowledgebase
)

;; Action to dunk the package.
(:action move
	:parameters (?i ?j - pos)
	:precondition (and
		(not (resolve-axioms))
		(adj ?i ?j)
		;; For every state ?s
		(Rat ?i basic)
		(Rat ?i kb_location_ball_0_loc_0)
		(Rat ?i kb_location_ball_0_loc_0_loc_1)
		(Rat ?i kb_colour_ball_0_colour_0)
		(Rat ?i kb_colour_ball_0_colour_0_colour_1)
		(Rat ?i kb_location_ball_1_loc_0)
		(Rat ?i kb_location_ball_1_loc_0_loc_1)
		(Rat ?i kb_colour_ball_1_colour_0)
		(Rat ?i kb_colour_ball_1_colour_0_colour_1)
	)
	:effect (and
		;; For every state ?s
		(when (m basic)
			(and
				(not (at ?i basic))
				(not (Rat ?i basic))
				(at ?j basic)
				(Rat ?j basic)
			)
		)
		(when (m kb_location_ball_0_loc_0)
			(and
				(not (at ?i kb_location_ball_0_loc_0))
				(not (Rat ?i kb_location_ball_0_loc_0))
				(at ?j kb_location_ball_0_loc_0)
				(Rat ?j kb_location_ball_0_loc_0)
			)
		)
		(when (m kb_location_ball_0_loc_0_loc_1)
			(and
				(not (at ?i kb_location_ball_0_loc_0_loc_1))
				(not (Rat ?i kb_location_ball_0_loc_0_loc_1))
				(at ?j kb_location_ball_0_loc_0_loc_1)
				(Rat ?j kb_location_ball_0_loc_0_loc_1)
			)
		)
		(when (m kb_colour_ball_0_colour_0)
			(and
				(not (at ?i kb_colour_ball_0_colour_0))
				(not (Rat ?i kb_colour_ball_0_colour_0))
				(at ?j kb_colour_ball_0_colour_0)
				(Rat ?j kb_colour_ball_0_colour_0)
			)
		)
		(when (m kb_colour_ball_0_colour_0_colour_1)
			(and
				(not (at ?i kb_colour_ball_0_colour_0_colour_1))
				(not (Rat ?i kb_colour_ball_0_colour_0_colour_1))
				(at ?j kb_colour_ball_0_colour_0_colour_1)
				(Rat ?j kb_colour_ball_0_colour_0_colour_1)
			)
		)
		(when (m kb_location_ball_1_loc_0)
			(and
				(not (at ?i kb_location_ball_1_loc_0))
				(not (Rat ?i kb_location_ball_1_loc_0))
				(at ?j kb_location_ball_1_loc_0)
				(Rat ?j kb_location_ball_1_loc_0)
			)
		)
		(when (m kb_location_ball_1_loc_0_loc_1)
			(and
				(not (at ?i kb_location_ball_1_loc_0_loc_1))
				(not (Rat ?i kb_location_ball_1_loc_0_loc_1))
				(at ?j kb_location_ball_1_loc_0_loc_1)
				(Rat ?j kb_location_ball_1_loc_0_loc_1)
			)
		)
		(when (m kb_colour_ball_1_colour_0)
			(and
				(not (at ?i kb_colour_ball_1_colour_0))
				(not (Rat ?i kb_colour_ball_1_colour_0))
				(at ?j kb_colour_ball_1_colour_0)
				(Rat ?j kb_colour_ball_1_colour_0)
			)
		)
		(when (m kb_colour_ball_1_colour_0_colour_1)
			(and
				(not (at ?i kb_colour_ball_1_colour_0_colour_1))
				(not (Rat ?i kb_colour_ball_1_colour_0_colour_1))
				(at ?j kb_colour_ball_1_colour_0_colour_1)
				(Rat ?j kb_colour_ball_1_colour_0_colour_1)
			)
		)
	)
)

;; Pick a ball up.
(:action pickup
	:parameters (?o - obj ?i - pos)
	:precondition (and
		(not (resolve-axioms))
		(Rat ?i basic)
		(Robj-at ?o ?i basic)
		(Rat ?i kb_location_ball_0_loc_0)
		(Robj-at ?o ?i kb_location_ball_0_loc_0)
		(Rat ?i kb_location_ball_0_loc_0_loc_1)
		(Robj-at ?o ?i kb_location_ball_0_loc_0_loc_1)
		(Rat ?i kb_colour_ball_0_colour_0)
		(Robj-at ?o ?i kb_colour_ball_0_colour_0)
		(Rat ?i kb_colour_ball_0_colour_0_colour_1)
		(Robj-at ?o ?i kb_colour_ball_0_colour_0_colour_1)
		(Rat ?i kb_location_ball_1_loc_0)
		(Robj-at ?o ?i kb_location_ball_1_loc_0)
		(Rat ?i kb_location_ball_1_loc_0_loc_1)
		(Robj-at ?o ?i kb_location_ball_1_loc_0_loc_1)
		(Rat ?i kb_colour_ball_1_colour_0)
		(Robj-at ?o ?i kb_colour_ball_1_colour_0)
		(Rat ?i kb_colour_ball_1_colour_0_colour_1)
		(Robj-at ?o ?i kb_colour_ball_1_colour_0_colour_1)
	)
	:effect (and
		;; For every state basic
		(when (m basic)
			(and
				(holding ?o basic)
				(Rholding ?o basic)
				(not (obj-at ?o ?i basic))
				(not (Robj-at ?o ?i basic))
			)
		)
		;; For every state kb_location_ball_0_loc_0
		(when (m kb_location_ball_0_loc_0)
			(and
				(holding ?o kb_location_ball_0_loc_0)
				(Rholding ?o kb_location_ball_0_loc_0)
				(not (obj-at ?o ?i kb_location_ball_0_loc_0))
				(not (Robj-at ?o ?i kb_location_ball_0_loc_0))
			)
		)
		;; For every state kb_location_ball_0_loc_0_loc_1
		(when (m kb_location_ball_0_loc_0_loc_1)
			(and
				(holding ?o kb_location_ball_0_loc_0_loc_1)
				(Rholding ?o kb_location_ball_0_loc_0_loc_1)
				(not (obj-at ?o ?i kb_location_ball_0_loc_0_loc_1))
				(not (Robj-at ?o ?i kb_location_ball_0_loc_0_loc_1))
			)
		)
		;; For every state kb_colour_ball_0_colour_0
		(when (m kb_colour_ball_0_colour_0)
			(and
				(holding ?o kb_colour_ball_0_colour_0)
				(Rholding ?o kb_colour_ball_0_colour_0)
				(not (obj-at ?o ?i kb_colour_ball_0_colour_0))
				(not (Robj-at ?o ?i kb_colour_ball_0_colour_0))
			)
		)
		;; For every state kb_colour_ball_0_colour_0_colour_1
		(when (m kb_colour_ball_0_colour_0_colour_1)
			(and
				(holding ?o kb_colour_ball_0_colour_0_colour_1)
				(Rholding ?o kb_colour_ball_0_colour_0_colour_1)
				(not (obj-at ?o ?i kb_colour_ball_0_colour_0_colour_1))
				(not (Robj-at ?o ?i kb_colour_ball_0_colour_0_colour_1))
			)
		)
		;; For every state kb_location_ball_1_loc_0
		(when (m kb_location_ball_1_loc_0)
			(and
				(holding ?o kb_location_ball_1_loc_0)
				(Rholding ?o kb_location_ball_1_loc_0)
				(not (obj-at ?o ?i kb_location_ball_1_loc_0))
				(not (Robj-at ?o ?i kb_location_ball_1_loc_0))
			)
		)
		;; For every state kb_location_ball_1_loc_0_loc_1
		(when (m kb_location_ball_1_loc_0_loc_1)
			(and
				(holding ?o kb_location_ball_1_loc_0_loc_1)
				(Rholding ?o kb_location_ball_1_loc_0_loc_1)
				(not (obj-at ?o ?i kb_location_ball_1_loc_0_loc_1))
				(not (Robj-at ?o ?i kb_location_ball_1_loc_0_loc_1))
			)
		)
		;; For every state kb_colour_ball_1_colour_0
		(when (m kb_colour_ball_1_colour_0)
			(and
				(holding ?o kb_colour_ball_1_colour_0)
				(Rholding ?o kb_colour_ball_1_colour_0)
				(not (obj-at ?o ?i kb_colour_ball_1_colour_0))
				(not (Robj-at ?o ?i kb_colour_ball_1_colour_0))
			)
		)
		;; For every state kb_colour_ball_1_colour_0_colour_1
		(when (m kb_colour_ball_1_colour_0_colour_1)
			(and
				(holding ?o kb_colour_ball_1_colour_0_colour_1)
				(Rholding ?o kb_colour_ball_1_colour_0_colour_1)
				(not (obj-at ?o ?i kb_colour_ball_1_colour_0_colour_1))
				(not (Robj-at ?o ?i kb_colour_ball_1_colour_0_colour_1))
			)
		)
	)
)

;; Trash a ball.
(:action trash
	:parameters (?o - obj ?c - col ?t -gar ?p - pos)
	:precondition (and
		(not (resolve-axioms))
		(Rcolor ?o ?c basic)
		(Rholding ?o basic)
		(Rat ?p basic)
		(garbage-at ?t ?p)
		(garbage-color ?t ?c)
		(Rcolor ?o ?c kb_location_ball_0_loc_0)
		(Rholding ?o kb_location_ball_0_loc_0)
		(Rat ?p kb_location_ball_0_loc_0)
		(garbage-at ?t ?p)
		(garbage-color ?t ?c)
		(Rcolor ?o ?c kb_location_ball_0_loc_0_loc_1)
		(Rholding ?o kb_location_ball_0_loc_0_loc_1)
		(Rat ?p kb_location_ball_0_loc_0_loc_1)
		(garbage-at ?t ?p)
		(garbage-color ?t ?c)
		(Rcolor ?o ?c kb_colour_ball_0_colour_0)
		(Rholding ?o kb_colour_ball_0_colour_0)
		(Rat ?p kb_colour_ball_0_colour_0)
		(garbage-at ?t ?p)
		(garbage-color ?t ?c)
		(Rcolor ?o ?c kb_colour_ball_0_colour_0_colour_1)
		(Rholding ?o kb_colour_ball_0_colour_0_colour_1)
		(Rat ?p kb_colour_ball_0_colour_0_colour_1)
		(garbage-at ?t ?p)
		(garbage-color ?t ?c)
		(Rcolor ?o ?c kb_location_ball_1_loc_0)
		(Rholding ?o kb_location_ball_1_loc_0)
		(Rat ?p kb_location_ball_1_loc_0)
		(garbage-at ?t ?p)
		(garbage-color ?t ?c)
		(Rcolor ?o ?c kb_location_ball_1_loc_0_loc_1)
		(Rholding ?o kb_location_ball_1_loc_0_loc_1)
		(Rat ?p kb_location_ball_1_loc_0_loc_1)
		(garbage-at ?t ?p)
		(garbage-color ?t ?c)
		(Rcolor ?o ?c kb_colour_ball_1_colour_0)
		(Rholding ?o kb_colour_ball_1_colour_0)
		(Rat ?p kb_colour_ball_1_colour_0)
		(garbage-at ?t ?p)
		(garbage-color ?t ?c)
		(Rcolor ?o ?c kb_colour_ball_1_colour_0_colour_1)
		(Rholding ?o kb_colour_ball_1_colour_0_colour_1)
		(Rat ?p kb_colour_ball_1_colour_0_colour_1)
		(garbage-at ?t ?p)
		(garbage-color ?t ?c)
	)
	:effect (and
		;; For every state basic
		(when (m basic)
			(and
				(trashed ?o basic)
				(Rtrashed ?o basic)
				(not (holding ?o basic))
				(not (Rholding ?o basic))
			)
		)
		;; For every state kb_location_ball_0_loc_0
		(when (m kb_location_ball_0_loc_0)
			(and
				(trashed ?o kb_location_ball_0_loc_0)
				(Rtrashed ?o kb_location_ball_0_loc_0)
				(not (holding ?o kb_location_ball_0_loc_0))
				(not (Rholding ?o kb_location_ball_0_loc_0))
			)
		)
		;; For every state kb_location_ball_0_loc_0_loc_1
		(when (m kb_location_ball_0_loc_0_loc_1)
			(and
				(trashed ?o kb_location_ball_0_loc_0_loc_1)
				(Rtrashed ?o kb_location_ball_0_loc_0_loc_1)
				(not (holding ?o kb_location_ball_0_loc_0_loc_1))
				(not (Rholding ?o kb_location_ball_0_loc_0_loc_1))
			)
		)
		;; For every state kb_colour_ball_0_colour_0
		(when (m kb_colour_ball_0_colour_0)
			(and
				(trashed ?o kb_colour_ball_0_colour_0)
				(Rtrashed ?o kb_colour_ball_0_colour_0)
				(not (holding ?o kb_colour_ball_0_colour_0))
				(not (Rholding ?o kb_colour_ball_0_colour_0))
			)
		)
		;; For every state kb_colour_ball_0_colour_0_colour_1
		(when (m kb_colour_ball_0_colour_0_colour_1)
			(and
				(trashed ?o kb_colour_ball_0_colour_0_colour_1)
				(Rtrashed ?o kb_colour_ball_0_colour_0_colour_1)
				(not (holding ?o kb_colour_ball_0_colour_0_colour_1))
				(not (Rholding ?o kb_colour_ball_0_colour_0_colour_1))
			)
		)
		;; For every state kb_location_ball_1_loc_0
		(when (m kb_location_ball_1_loc_0)
			(and
				(trashed ?o kb_location_ball_1_loc_0)
				(Rtrashed ?o kb_location_ball_1_loc_0)
				(not (holding ?o kb_location_ball_1_loc_0))
				(not (Rholding ?o kb_location_ball_1_loc_0))
			)
		)
		;; For every state kb_location_ball_1_loc_0_loc_1
		(when (m kb_location_ball_1_loc_0_loc_1)
			(and
				(trashed ?o kb_location_ball_1_loc_0_loc_1)
				(Rtrashed ?o kb_location_ball_1_loc_0_loc_1)
				(not (holding ?o kb_location_ball_1_loc_0_loc_1))
				(not (Rholding ?o kb_location_ball_1_loc_0_loc_1))
			)
		)
		;; For every state kb_colour_ball_1_colour_0
		(when (m kb_colour_ball_1_colour_0)
			(and
				(trashed ?o kb_colour_ball_1_colour_0)
				(Rtrashed ?o kb_colour_ball_1_colour_0)
				(not (holding ?o kb_colour_ball_1_colour_0))
				(not (Rholding ?o kb_colour_ball_1_colour_0))
			)
		)
		;; For every state kb_colour_ball_1_colour_0_colour_1
		(when (m kb_colour_ball_1_colour_0_colour_1)
			(and
				(trashed ?o kb_colour_ball_1_colour_0_colour_1)
				(Rtrashed ?o kb_colour_ball_1_colour_0_colour_1)
				(not (holding ?o kb_colour_ball_1_colour_0_colour_1))
				(not (Rholding ?o kb_colour_ball_1_colour_0_colour_1))
			)
		)
	)
)

;; Sense the color.
(:action observe-color
	:parameters (?c - col ?o - obj ?l ?l2 - level ?kb - knowledgebase)
	:precondition (and
		(not (resolve-axioms))
		(next ?l ?l2)
		(lev ?l)
		(current_kb ?kb)

		(Rholding ?o basic)
		(Rholding ?o kb_location_ball_0_loc_0)
		(Rholding ?o kb_location_ball_0_loc_0_loc_1)
		(Rholding ?o kb_colour_ball_0_colour_0)
		(Rholding ?o kb_colour_ball_0_colour_0_colour_1)
		(Rholding ?o kb_location_ball_1_loc_0)
		(Rholding ?o kb_location_ball_1_loc_0_loc_1)
		(Rholding ?o kb_colour_ball_1_colour_0)
		(Rholding ?o kb_colour_ball_1_colour_0_colour_1)
		;; This action is only applicable if there are world states where the outcome can be different.
		(exists (?s - state) (and (m ?s) (color ?o ?c ?s) (part-of ?s ?kb)))
		(exists (?s - state) (and (m ?s) (not (color ?o ?c ?s)) (part-of ?s ?kb)))
	)
	:effect (and
		(not (lev ?l))
		(lev ?l2)

		;; For every state ?s
		(when (and (m basic) (not (color ?o ?c basic)))
			(and (stack basic ?l) (not (m basic)))
		)
		(when (and (m kb_location_ball_0_loc_0) (not (color ?o ?c kb_location_ball_0_loc_0)))
			(and (stack kb_location_ball_0_loc_0 ?l) (not (m kb_location_ball_0_loc_0)))
		)
		(when (and (m kb_location_ball_0_loc_0_loc_1) (not (color ?o ?c kb_location_ball_0_loc_0_loc_1)))
			(and (stack kb_location_ball_0_loc_0_loc_1 ?l) (not (m kb_location_ball_0_loc_0_loc_1)))
		)
		(when (and (m kb_colour_ball_0_colour_0) (not (color ?o ?c kb_colour_ball_0_colour_0)))
			(and (stack kb_colour_ball_0_colour_0 ?l) (not (m kb_colour_ball_0_colour_0)))
		)
		(when (and (m kb_colour_ball_0_colour_0_colour_1) (not (color ?o ?c kb_colour_ball_0_colour_0_colour_1)))
			(and (stack kb_colour_ball_0_colour_0_colour_1 ?l) (not (m kb_colour_ball_0_colour_0_colour_1)))
		)
		(when (and (m kb_location_ball_1_loc_0) (not (color ?o ?c kb_location_ball_1_loc_0)))
			(and (stack kb_location_ball_1_loc_0 ?l) (not (m kb_location_ball_1_loc_0)))
		)
		(when (and (m kb_location_ball_1_loc_0_loc_1) (not (color ?o ?c kb_location_ball_1_loc_0_loc_1)))
			(and (stack kb_location_ball_1_loc_0_loc_1 ?l) (not (m kb_location_ball_1_loc_0_loc_1)))
		)
		(when (and (m kb_colour_ball_1_colour_0) (not (color ?o ?c kb_colour_ball_1_colour_0)))
			(and (stack kb_colour_ball_1_colour_0 ?l) (not (m kb_colour_ball_1_colour_0)))
		)
		(when (and (m kb_colour_ball_1_colour_0_colour_1) (not (color ?o ?c kb_colour_ball_1_colour_0_colour_1)))
			(and (stack kb_colour_ball_1_colour_0_colour_1 ?l) (not (m kb_colour_ball_1_colour_0_colour_1)))
		)
		(resolve-axioms)
	)
)

;; Sense the ball.
(:action observe-ball
	:parameters (?pos - pos ?o - obj ?l ?l2 - level ?kb - knowledgebase)
	:precondition (and
		(not (resolve-axioms))
		(next ?l ?l2)
		(lev ?l)
		(current_kb ?kb)

		(Rat ?pos basic)
		(Rat ?pos kb_location_ball_0_loc_0)
		(Rat ?pos kb_location_ball_0_loc_0_loc_1)
		(Rat ?pos kb_colour_ball_0_colour_0)
		(Rat ?pos kb_colour_ball_0_colour_0_colour_1)
		(Rat ?pos kb_location_ball_1_loc_0)
		(Rat ?pos kb_location_ball_1_loc_0_loc_1)
		(Rat ?pos kb_colour_ball_1_colour_0)
		(Rat ?pos kb_colour_ball_1_colour_0_colour_1)
		;; This action is only applicable if there are world states where the outcome can be different.
		(exists (?s - state) (and (m ?s) (obj-at ?o ?pos ?s) (part-of ?s ?kb)))
		(exists (?s - state) (and (m ?s) (not (obj-at ?o ?pos ?s)) (part-of ?s ?kb)))
	)
	:effect (and
		(not (lev ?l))
		(lev ?l2)

		;; For every state ?s
		(when (and (m basic) (not (obj-at ?o ?pos basic)))
			(and (stack basic ?l) (not (m basic)))
		)
		(when (and (m kb_location_ball_0_loc_0) (not (obj-at ?o ?pos kb_location_ball_0_loc_0)))
			(and (stack kb_location_ball_0_loc_0 ?l) (not (m kb_location_ball_0_loc_0)))
		)
		(when (and (m kb_location_ball_0_loc_0_loc_1) (not (obj-at ?o ?pos kb_location_ball_0_loc_0_loc_1)))
			(and (stack kb_location_ball_0_loc_0_loc_1 ?l) (not (m kb_location_ball_0_loc_0_loc_1)))
