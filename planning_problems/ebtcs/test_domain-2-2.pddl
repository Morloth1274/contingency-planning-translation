(define (domain find_key)
(:requirements :typing :conditional-effects :negative-preconditions :disjunctive-preconditions)

(:types
	package bomb toilet
	level
	state
	knowledgebase
)

(:predicates
	(in ?p - package ?b - bomb ?s - state)
	(Rin ?p - package ?b - bomb ?s - state)
	(defused ?b - bomb ?s - state)
	(Rdefused ?b - bomb ?s - state)
	(nclog ?t - toilet ?s - state)
	(Rnclog ?t - toilet ?s - state)
	(part-of ?s - state ?kb - knowledgebase)
	(current_kb ?kb - knowledgebase)
	(parent ?kb ?kb2 - knowledgebase)

	;; Bookkeeping predicates.
	(next ?l ?l2 - level)
	(lev ?l - LEVEL)
	(m ?s - STATE)
	(stack ?s - STATE ?l - LEVEL)
	(resolve-axioms)
)

(:constants
	; All the packages.
	package_0 - package
	package_1 - package
	; The bombs.
	bomb_0 - bomb
	bomb_1 - bomb
	; The toilet.
	toilet - toilet
	; All the states.
	basic - state
	kb_bomb_0_package_0 - state
	kb_bomb_0_package_0_package_1 - state
	kb_bomb_1_package_0 - state
	kb_bomb_1_package_0_package_1 - state
	; The knowledge bases
	basis_kb - knowledgebase
	kb_bomb_0 - knowledgebase
	kb_bomb_1 - knowledgebase
)

;; Action to dunk the package.
(:action dunk
	:parameters (?p - package ?b - bomb ?t - toilet)
	:precondition (and
		(not (resolve-axioms))
		;; For every state ?s
		(Rnclog ?t basic)
		(Rin ?p ?b basic)
		(Rnclog ?t kb_bomb_0_package_0)
		(Rin ?p ?b kb_bomb_0_package_0)
		(Rnclog ?t kb_bomb_0_package_0_package_1)
		(Rin ?p ?b kb_bomb_0_package_0_package_1)
		(Rnclog ?t kb_bomb_1_package_0)
		(Rin ?p ?b kb_bomb_1_package_0)
		(Rnclog ?t kb_bomb_1_package_0_package_1)
		(Rin ?p ?b kb_bomb_1_package_0_package_1)
	)
	:effect (and
		;; For every state ?s
		(when (m basic)
			(and
				(not (nclog ?t basic))
				(not (Rnclog ?t basic))
				(defused ?b basic)
				(Rdefused ?b basic)
			)
		)
		(when (m kb_bomb_0_package_0)
			(and
				(not (nclog ?t kb_bomb_0_package_0))
				(not (Rnclog ?t kb_bomb_0_package_0))
				(defused ?b kb_bomb_0_package_0)
				(Rdefused ?b kb_bomb_0_package_0)
			)
		)
		(when (m kb_bomb_0_package_0_package_1)
			(and
				(not (nclog ?t kb_bomb_0_package_0_package_1))
				(not (Rnclog ?t kb_bomb_0_package_0_package_1))
				(defused ?b kb_bomb_0_package_0_package_1)
				(Rdefused ?b kb_bomb_0_package_0_package_1)
			)
		)
		(when (m kb_bomb_1_package_0)
			(and
				(not (nclog ?t kb_bomb_1_package_0))
				(not (Rnclog ?t kb_bomb_1_package_0))
				(defused ?b kb_bomb_1_package_0)
				(Rdefused ?b kb_bomb_1_package_0)
			)
		)
		(when (m kb_bomb_1_package_0_package_1)
			(and
				(not (nclog ?t kb_bomb_1_package_0_package_1))
				(not (Rnclog ?t kb_bomb_1_package_0_package_1))
				(defused ?b kb_bomb_1_package_0_package_1)
				(Rdefused ?b kb_bomb_1_package_0_package_1)
			)
		)
	)
)

;; Flush a toilet.
(:action flush
	:parameters (?t - toilet)
	:precondition (and
		(not (resolve-axioms))
	)
	:effect (and
		;; For every state basic
		(when (m basic)
			(and
				(nclog ?t basic)
				(Rnclog ?t basic)
			)
		)
		;; For every state kb_bomb_0_package_0
		(when (m kb_bomb_0_package_0)
			(and
				(nclog ?t kb_bomb_0_package_0)
				(Rnclog ?t kb_bomb_0_package_0)
			)
		)
		;; For every state kb_bomb_0_package_0_package_1
		(when (m kb_bomb_0_package_0_package_1)
			(and
				(nclog ?t kb_bomb_0_package_0_package_1)
				(Rnclog ?t kb_bomb_0_package_0_package_1)
			)
		)
		;; For every state kb_bomb_1_package_0
		(when (m kb_bomb_1_package_0)
			(and
				(nclog ?t kb_bomb_1_package_0)
				(Rnclog ?t kb_bomb_1_package_0)
			)
		)
		;; For every state kb_bomb_1_package_0_package_1
		(when (m kb_bomb_1_package_0_package_1)
			(and
				(nclog ?t kb_bomb_1_package_0_package_1)
				(Rnclog ?t kb_bomb_1_package_0_package_1)
			)
		)
	)
)

;; Sense for the bomb.
(:action sense
	:parameters (?p - package ?b - bomb ?l ?l2 - level ?kb - knowledgebase)
	:precondition (and
		(not (resolve-axioms))
		(next ?l ?l2)
		(lev ?l)
		(current_kb ?kb)

		;; This action is only applicable if there are world states where the outcome can be different.
		(exists (?s - state) (and (m ?s) (in ?p ?b ?s) (part-of ?s ?kb)))
		(exists (?s - state) (and (m ?s) (not (in ?p ?b ?s)) (part-of ?s ?kb)))
	)
	:effect (and
		(not (lev ?l))
		(lev ?l2)

		;; For every state ?s
		(when (and (m basic) (not (in ?p ?b basic)))
			(and (stack basic ?l) (not (m basic)))
		)
		(when (and (m kb_bomb_0_package_0) (not (in ?p ?b kb_bomb_0_package_0)))
			(and (stack kb_bomb_0_package_0 ?l) (not (m kb_bomb_0_package_0)))
		)
		(when (and (m kb_bomb_0_package_0_package_1) (not (in ?p ?b kb_bomb_0_package_0_package_1)))
			(and (stack kb_bomb_0_package_0_package_1 ?l) (not (m kb_bomb_0_package_0_package_1)))
		)
		(when (and (m kb_bomb_1_package_0) (not (in ?p ?b kb_bomb_1_package_0)))
			(and (stack kb_bomb_1_package_0 ?l) (not (m kb_bomb_1_package_0)))
		)
		(when (and (m kb_bomb_1_package_0_package_1) (not (in ?p ?b kb_bomb_1_package_0_package_1)))
			(and (stack kb_bomb_1_package_0_package_1 ?l) (not (m kb_bomb_1_package_0_package_1)))
		)
		(resolve-axioms)
	)
)

;; Exit the current branch.
(:action pop
	:parameters (?l ?l2 - level)
	:precondition (and
		(lev ?l)
		(next ?l2 ?l)
		(not (resolve-axioms))
	)
	:effect (and 
		(not (lev ?l))
		(lev ?l2)
		(resolve-axioms)
		(when (m basic) 
			(not (m basic))
		)
		(when (stack basic ?l2)
			(and 
				(m basic)
				(not (stack basic ?l2))
			)
		)
		(when (m kb_bomb_0_package_0) 
			(not (m kb_bomb_0_package_0))
		)
		(when (stack kb_bomb_0_package_0 ?l2)
			(and 
				(m kb_bomb_0_package_0)
				(not (stack kb_bomb_0_package_0 ?l2))
			)
		)
		(when (m kb_bomb_0_package_0_package_1) 
			(not (m kb_bomb_0_package_0_package_1))
		)
		(when (stack kb_bomb_0_package_0_package_1 ?l2)
			(and 
				(m kb_bomb_0_package_0_package_1)
				(not (stack kb_bomb_0_package_0_package_1 ?l2))
			)
		)
		(when (m kb_bomb_1_package_0) 
			(not (m kb_bomb_1_package_0))
		)
		(when (stack kb_bomb_1_package_0 ?l2)
			(and 
				(m kb_bomb_1_package_0)
				(not (stack kb_bomb_1_package_0 ?l2))
			)
		)
		(when (m kb_bomb_1_package_0_package_1) 
			(not (m kb_bomb_1_package_0_package_1))
		)
		(when (stack kb_bomb_1_package_0_package_1 ?l2)
			(and 
				(m kb_bomb_1_package_0_package_1)
				(not (stack kb_bomb_1_package_0_package_1 ?l2))
			)
		)
	)
)

;; Resolve the axioms manually.
(:action raminificate
	:parameters ()
	:precondition (resolve-axioms)
	:effect (and 
		(not (resolve-axioms))
		;; For every state ?s
		(when (or (defused bomb_0 basic) (not (m basic)))
			(Rdefused bomb_0 basic)
		)
		(when (and (not (defused bomb_0 basic)) (m basic))
			(not (Rdefused bomb_0 basic))
		)
		(when (or (in package_0 bomb_0 basic) (not (m basic)))
			(Rin package_0 bomb_0 basic)
		)
		(when (and (not (in package_0 bomb_0 basic)) (m basic))
			(not (Rin package_0 bomb_0 basic))
		)
		(when (or (in package_1 bomb_0 basic) (not (m basic)))
			(Rin package_1 bomb_0 basic)
		)
		(when (and (not (in package_1 bomb_0 basic)) (m basic))
			(not (Rin package_1 bomb_0 basic))
		)
		(when (or (defused bomb_1 basic) (not (m basic)))
			(Rdefused bomb_1 basic)
		)
		(when (and (not (defused bomb_1 basic)) (m basic))
			(not (Rdefused bomb_1 basic))
		)
		(when (or (in package_0 bomb_1 basic) (not (m basic)))
			(Rin package_0 bomb_1 basic)
		)
		(when (and (not (in package_0 bomb_1 basic)) (m basic))
			(not (Rin package_0 bomb_1 basic))
		)
		(when (or (in package_1 bomb_1 basic) (not (m basic)))
