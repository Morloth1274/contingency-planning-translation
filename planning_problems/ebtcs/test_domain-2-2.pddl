		)
		(when (and (not (defused bomb_0 kb_bomb_0_package_0)) (m kb_bomb_0_package_0))
			(not (Rdefused bomb_0 kb_bomb_0_package_0))
		)
		(when (or (in package_0 bomb_0 kb_bomb_0_package_0) (not (m kb_bomb_0_package_0)))
